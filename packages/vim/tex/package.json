{
  "description": "Vim support for (La)TeX code editing",
  "dependencies": [
    "vim.pathogen"
  ],
  "prepare": [
    {
      "kind": "git clone",
      "remote": "http://github.com/lervag/vimtex"
    }
  ],
  "install": [
    {
      "kind": "copy tree",
      "folder": "$TEMPORARY_DIR/vimtex",
      "to": "$HOME/.vim/bundle/vimtex"
    }
  ]
}
