{
  "description": "Vim status line",
  "dependencies": [
    "vim.pathogen"
  ],
  "prepare": [
    {
      "kind": "git clone",
      "remote": "http://github.com/vim-airline/vim-airline"
    }
  ],
  "install": [
    {
      "kind": "copy tree",
      "folder": "$TEMPORARY_DIR/vim-airline",
      "to": "$HOME/.vim/bundle/vim-airline"
    },
    {
      "kind": "copy",
      "file": "config.vim",
      "to": "$HOME/.vim/config/vim-airline.vim"
    }
  ]
}
