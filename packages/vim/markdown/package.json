{
  "description": "Vim Markdown support",
  "dependencies": [
    "vim.pathogen"
  ],
  "prepare": [
    {
      "kind": "git clone",
      "remote": "http://github.com/plasticboy/vim-markdown"
    }
  ],
  "install": [
    {
      "kind": "copy tree",
      "folder": "$TEMPORARY_DIR/vim-markdown",
      "to": "$HOME/.vim/bundle/vim-markdown"
    }
  ]
}
